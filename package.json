{
  "name": "duxanator",
  "version": "3.0.0",
  "description": "simple state store for react",
  "main": "dist/main.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist",
    "build:watch": "babel ./src --out-dir ./dist --watch",
    "test": "mocha --opts test/mocha.opts --compilers js:babel-register",
