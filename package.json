{
  "name": "duxanator",
  "version": "1.0.1",
  "description": "simple state store for react",
  "main": "dist/main.js",
  "scripts": {
    "prepublish": "npm run test && npm run build; git commit -a -m \"prepublish build\"",
    "build": "babel ./src --out-dir ./dist",
    "build:watch": "babel ./src --out-dir ./dist --watch",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register -w"
  },
  "author": "Adam Beck",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.8.2",
    "react": "^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3"
  }
}
